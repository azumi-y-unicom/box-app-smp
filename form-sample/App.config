<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2" />
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Buffers" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.3.0" newVersion="4.0.3.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
	<appSettings>
		<!-- 必須：JWT認証用JSONファイルのパス -->
		<add key="auth-json-file-path" value="./json/config.json" />
		<!-- 必須：Boxの仕様として0からスタートする -->
		<add key="box-root-folder-id" value="0" />
		<!-- 必須：アップロードの時に分割アップロードするかしないかの閾値；[50MB]が公式の推奨値 -->
		<add key="upload-file-size-threshold" value="50000000" />
	</appSettings>
</configuration>